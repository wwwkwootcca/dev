source 'https://rubygems.org'

# Make authorizations / connections happy - provide credentials.
gem 'netrc'

# Initial theme was jekyll-theme-slate
#gem "jekyll-theme-slate"
# minima is a theme.
gem "minima"

# Classifier classifies text. For LSI, presumably to help (google) search results.
gem 'classifier-reborn'

gem 'github-pages', group: :jekyll_plugins

gem 'jekyll-github-metadata'

# Feed - help RSS' with atom.xml's.
gem 'jekyll-feed'

# Mentions - make links, email, twitter, etc., pretty / links.
gem 'jekyll-mentions'

gem 'jekyll-seo-tag'

gem 'jekyll-tidy'

gem 'html-proofer'
